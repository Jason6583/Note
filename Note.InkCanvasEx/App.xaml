    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Note.InkCanvasEx"
    xmlns:controls="using:Note.InkCanvasEx.Controls"
    xmlns:xaml="using:Microsoft.Toolkit.Win32.UI.XamlHost">
    <xaml:XamlApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Themes/Images.xaml"/>
                <ResourceDictionary Source="ms-appx:///Themes/ButtonStyles.xaml"/>
                <ResourceDictionary Source="ms-appx:///Themes/FlyoutStyles.xaml"/>                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </xaml:XamlApplication.Resources>
</xaml:XamlApplication>
